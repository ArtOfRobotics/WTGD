<?xml version="1.0"?>

<launch>
  <param name="map_frame" value="map"/> 
  <param name="base_frame" value="base_link"/> 
  <param name="odom_frame" value="base_link"/>
  
  <node pkg="sick_tim" type="sick_tim551_2050001" name="sick_tim551_2050001"/>
  <node pkg="tf" type="static_transform_publisher" name="base_frame_2_laser_link" args="0.3 0 0 0 0 0 /base_link /laser 100"/> 
  
  <node pkg="rviz" type="rviz" name="rviz" args="-d $(find navigation2d_example)/rviz_cfg/rviz_cfg.rviz"/>

  <include file="$(find navigation2d_example)/launch/default_mapping.launch"> 
    <arg name="base_frame" value="base_link"/>
    <arg name="odom_frame" value="base_link"/>
  </include>
  <include file="$(find hector_geotiff)/launch/geotiff_mapper.launch">
    <arg name="trajectory_source_frame_name" value="/base_link"/>
  </include>

</launch>
